<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Amostra.
    -->
    <changeSet id="20210326090946-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="operacao_id"
                                 baseTableName="amostra"
                                 constraintName="fk_amostra_operacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="operacao"/>

        <addForeignKeyConstraint baseColumnNames="origem_amostra_id"
                                 baseTableName="amostra"
                                 constraintName="fk_amostra_origem_amostra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="origem_amostra"/>

        <addForeignKeyConstraint baseColumnNames="tipo_amostra_id"
                                 baseTableName="amostra"
                                 constraintName="fk_amostra_tipo_amostra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_amostra"/>

        <addForeignKeyConstraint baseColumnNames="finalidade_amostra_id"
                                 baseTableName="amostra"
                                 constraintName="fk_amostra_finalidade_amostra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="finalidade_amostra"/>

    </changeSet>
</databaseChangeLog>
