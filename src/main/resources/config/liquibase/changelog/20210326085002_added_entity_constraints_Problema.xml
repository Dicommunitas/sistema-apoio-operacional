<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Problema.
    -->
    <changeSet id="20210326085002-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="problema_id"
                                 baseTableName="problema"
                                 constraintName="fk_problema_problema_id"
                                 referencedColumnNames="id"
                                 referencedTableName="status"/>

        <addForeignKeyConstraint baseColumnNames="status_id"
                                 baseTableName="problema"
                                 constraintName="fk_problema_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="status"/>

    </changeSet>
</databaseChangeLog>
